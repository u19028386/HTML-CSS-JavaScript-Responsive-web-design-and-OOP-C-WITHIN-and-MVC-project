@model List<u19028386_HomeW04.Models.Bird>
@{
    ViewBag.Title = "DeleteBird";
}

<h2>Delete Bird</h2>
@using (Html.BeginForm("DeleteBird", "HelloAso", FormMethod.Post))
{
    <label for="name">Name</label>
    <input type="text" name="name" />
    <br />

    <label for="country">Country</label>
